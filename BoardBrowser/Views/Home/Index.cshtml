@{
    ViewBag.Title = "Home Page";
}
<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item active">
            <img class="center-block" src="https://cdn.shopify.com/s/files/1/2941/2202/products/PS21W0032-ITACHICROWSDECK_1024x1024_61c15b2f-82e0-46b4-b68c-30dea32d14a7_700x700.jpg?v=1616273967" alt="Primitive x Naruto Skateboard: Itachi">
        </div>

        <div class="item">
            <img class="center-block" src="https://i.pinimg.com/originals/1b/45/37/1b45377aaa0a7e9d0ad9aa11b3733fc4.jpg" alt="Landyatchz Longboard: Fox">
        </div>

        <div class="item">
            <img class="center-block" src="https://images.the-house.com/santacruz-lion-god-drop-thru-longboard-complete-16-zoom.jpg" alt="Santa Cruz Longboard: Lion">
        </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
